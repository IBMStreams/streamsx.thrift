#if !defined(STREAMS_BOOST_TR1_CMATH_INCLUDED) || defined(STREAMS_BOOST_TR1_NO_RECURSION)
#ifndef STREAMS_BOOST_TR1_CMATH_INCLUDED
#  define STREAMS_BOOST_TR1_CMATH_INCLUDED
#endif
#  ifdef STREAMS_BOOST_TR1_NO_CMATH_RECURSION2
#     define STREAMS_BOOST_TR1_NO_CMATH_RECURSION3
#  elif defined(STREAMS_BOOST_TR1_NO_CMATH_RECURSION)
#     define STREAMS_BOOST_TR1_NO_CMATH_RECURSION2
#  elif !defined(STREAMS_BOOST_TR1_NO_RECURSION)
#     define STREAMS_BOOST_TR1_NO_RECURSION
#     define STREAMS_BOOST_TR1_NO_CMATH_RECURSION
#  endif
#  include <streams_boost/tr1/detail/config_all.hpp>
#  ifdef STREAMS_BOOST_HAS_INCLUDE_NEXT
#     include_next <cmath>
#  else
#     include STREAMS_BOOST_TR1_STD_CHEADER(cmath)
#  endif
#ifdef STREAMS_BOOST_TR1_NO_CMATH_RECURSION3
#  undef STREAMS_BOOST_TR1_NO_CMATH_RECURSION3
#elif defined(STREAMS_BOOST_TR1_NO_CMATH_RECURSION2)
#  undef STREAMS_BOOST_TR1_NO_CMATH_RECURSION2
#elif defined(STREAMS_BOOST_TR1_NO_CMATH_RECURSION)
#     undef STREAMS_BOOST_TR1_NO_RECURSION
#     undef STREAMS_BOOST_TR1_NO_CMATH_RECURSION
#  endif
#endif

#if !defined(STREAMS_BOOST_TR1_FULL_CMATH_INCLUDED) && !defined(STREAMS_BOOST_TR1_NO_RECURSION)
#  define STREAMS_BOOST_TR1_FULL_CMATH_INCLUDED
#  define STREAMS_BOOST_TR1_NO_RECURSION
#  include <streams_boost/tr1/cmath.hpp>
#  undef STREAMS_BOOST_TR1_NO_RECURSION
#endif

