#ifndef STREAMS_BOOST_TR1_UNORDERED_MAP_INCLUDED
#  define STREAMS_BOOST_TR1_UNORDERED_MAP_INCLUDED
#  include <streams_boost/tr1/detail/config_all.hpp>

#  ifdef STREAMS_BOOST_HAS_CPP_0X
#     ifdef STREAMS_BOOST_HAS_INCLUDE_NEXT
#        include_next <unordered_map>
#     else
#        include STREAMS_BOOST_TR1_STD_HEADER(unordered_map)
#     endif
#  endif

#  if !defined(STREAMS_BOOST_TR1_NO_RECURSION)
#  define STREAMS_BOOST_TR1_NO_RECURSION
#  ifdef STREAMS_BOOST_HAS_TR1_UNORDERED_MAP
#     ifdef STREAMS_BOOST_HAS_INCLUDE_NEXT
#        include_next STREAMS_BOOST_TR1_HEADER(unordered_map)
#     else
#        include STREAMS_BOOST_TR1_STD_HEADER(STREAMS_BOOST_TR1_PATH(unordered_map))
#     endif
#  else
#     include <streams_boost/tr1/unordered_map.hpp>
#  endif
#  undef STREAMS_BOOST_TR1_NO_RECURSION
#  endif
#endif


